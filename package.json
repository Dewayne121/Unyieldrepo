{
  "name": "unyielding",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "set EXPO_OFFLINE=1&& expo start --localhost",
    "android": "set EXPO_OFFLINE=1&& expo start --android --localhost",
    "ios": "set EXPO_OFFLINE=1&& expo start --ios --localhost",
    "web": "expo start --web",
    "lan": "set EXPO_OFFLINE=1&& expo start --lan",
    "tunnel": "cmd /c \"expo start --tunnel --clear --port 8081 || (echo Retrying tunnel... & expo start --tunnel --clear --port 8081)\"",
    "postinstall": "node scripts/patchExpoNgrokTimeout.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.5.1",
    "@react-navigation/bottom-tabs": "^7.9.0",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.0",
    "bcryptjs": "^3.0.3",
    "expo": "~54.0.30",
    "expo-asset": "^12.0.12",
    "expo-av": "^16.0.8",
    "expo-camera": "^17.0.10",
    "expo-constants": "^18.0.7",
    "expo-device": "^7.0.3",
    "expo-haptics": "^15.0.8",
    "expo-image-picker": "^17.0.10",
    "expo-linear-gradient": "^15.0.8",
    "expo-notifications": "^0.32.15",
    "expo-status-bar": "~3.0.9",
    "expo-vector-icons": "^10.0.1",
    "expo-video-thumbnails": "~10.0.8",
    "oci-common": "^2.122.2",
    "oci-sdk": "^2.122.2",
    "pg": "^8.17.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "sharp": "^0.34.5"
  },
  "private": true,
  "devDependencies": {
    "@expo/ngrok": "^4.1.0",
    "eas-cli": "^16.32.0"
  }
